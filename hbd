def ngeloopHBD():
    i = 0
    while i < 13:
        print(
"""
H       H  B B B     D D D D    H       H      A      R R R R
H       H  B    B    D      D   H       H     A A     R    R
H H H H H  B B B B   D       D  H H H H H    A A A    R   R
H       H  B      B  D      D   H       H   A     A   R    R
H       H  B B B B   D D D D    H       H  A       A  R     R
""")
        i += 1

        
print("""
      WIII SIAPA NIH HARI INI ULTAH UWOW
      ok disclaimer dlu sy udah lupa sama bahasa ular ini,
      jadi mmfkn sy apabila codenya sangat dasar padahal dari seorang tif
      sama aku ga punya ide mau isi apa kodenya hehe jadi aku ulang hbdnya sampe 13 kali""")

print(ngeloopHBD())
